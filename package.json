{
  "name": "easymock",
  "displayName": "EasyMock",
  "description": "a simple mock server",
  "version": "1.0.0",
  "publisher": "EasyMock",
  "license": "MIT",
  "engines": {
    "vscode": "^1.21.0"
  },
  "icon": "asset/icon.png",
  "keywords": ["mock", "easy mock", "mock server"],
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:easymock.runMcok",
    "onCommand:easymock.stopMcok"
  ],
  "main": "./src/index",
  "contributes": {
    "commands": [
      {
        "command": "easymock.runMcok",
        "title": "Run EasyMock",
        "category": "EasyMock"
      },
      {
        "command": "easymock.stopMcok",
        "title": "Stop EasyMock",
        "category": "EasyMock"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Easy Mock configuration",
      "properties": {
        "EasyMock.serverPort": {
          "type": "number",
          "default": 9999,
          "description": "Set mock server port"
        },
        "EasyMock.mockFolderName": {
          "type": "string",
          "default": "mock",
          "description": "Specifies the mock folder name in your workspace"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "pack": "vsce package",
    "localinstall": "code --install-extension easymock-1.0.0.vsix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Elity/EasyMock.git"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "express-http-proxy": "^1.1.0",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "vscode": "^1.1.6"
  }
}
